@startuml
!theme plain
skinparam classAttributeIconSize 0

class User {
  -id: int
  -username: string
  -email: string
  +login()
  +logout()
}

class Product {
  -id: int
  -name: string
  -price: decimal
  +create()
  +update()
}

User ||--o{ Order : places
Product ||--o{ OrderItem : contains
@enduml